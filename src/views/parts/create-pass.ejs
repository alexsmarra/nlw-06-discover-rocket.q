<section>
   <h2>Crie sua própria sala</h2>
   <form class="form-create-pass" action="/create-room" method="post">
      <!-- Os label .sr-only não irão aparecer, mas são para facilitar na acessibilidade e em motores de busca (ex: google) -->
      <label class="sr-only" for="room-pass">Insira uma senha</label>
      <input type="password" name="password" id="room-pass" placeholder="Código da sala" />
      <button>
         <img src="./images/users-white.svg" alt="Entrar na sala" />
         Criar sala
      </button>
   </form>
</section>
